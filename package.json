{
  "name": "finalscalculator",
  "description": "Finals Calculator is a simple app that calculates your final score in a class. It is designed to be used in the Korean education system, but it can be used in other education systems as well",
  "version": "23.3",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hslee2008/finalscalcu.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run .",
    "test:watch": "vitest .",
    "update": "yarn upgrade && ncu -u && yarn",
    "build:android": "cd android && bubblewrap build",
    "lint": "eslint . --ext .ts,.js,.svelte",
    "firebase-hosting-build": "yarn && yarn test && yarn lint && yarn build"
  },
  "devDependencies": {
    "@beyonk/svelte-google-analytics": "^2.6.4",
    "@sveltejs/vite-plugin-svelte": "^2.1.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@typescript-eslint/parser": "^5.59.1",
    "carbon-components-svelte": "^0.73.5",
    "carbon-preprocess-svelte": "^0.9.1",
    "eslint": ">=8.39.0",
    "eslint-plugin-testing-library": "^5.10.3",
    "jsdom": "^21.1.1",
    "svelte": "^3.58.0",
    "svelte-eslint-parser": "^0.27.0",
    "svelte-meta-tags": "^2.7.0",
    "vite": "^4.3.3",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "firebase": "^9.21.0"
  }
}
