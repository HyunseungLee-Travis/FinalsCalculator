{
  "name": "finalscalculator",
  "description": "Finals Calculator is a simple app that calculates your final score in a class. It is designed to be used in the Korean education system, but it can be used in other education systems as well",
  "version": "23.3",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hslee2008/finalscalcu.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run .",
    "test:watch": "vitest .",
    "update": "yarn upgrade && ncu -u && yarn",
    "build:android": "cd android && bubblewrap build",
    "lint": "eslint . --ext .ts,.js,.svelte",
    "firebase-hosting-build": "yarn && yarn test && yarn lint && yarn build",
    "prep": "yarn install && npm install -g npm-check-updates",
    "go": "yarn prepare && yarn dev"
  },
  "devDependencies": {
    "@bubblewrap/cli": "^1.21.0",
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/svelte": "^4.0.5",
    "@typescript-eslint/parser": "^6.9.1",
    "carbon-components-svelte": "^0.81.2",
    "carbon-preprocess-svelte": "^0.10.0",
    "eslint": ">=8.53.0",
    "eslint-plugin-testing-library": "^6.1.0",
    "jsdom": "^22.1.0",
    "svelte": "^4.2.2",
    "svelte-eslint-parser": "^0.33.1",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@beyonk/svelte-google-analytics": "^2.6.4",
    "firebase": "^10.5.2",
    "svelte-meta-tags": "^3.0.4",
    "svelte-tabs": "^1.1.0"
  }
}
