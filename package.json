{
  "name": "finalscalculator",
  "description": "Finals Calculator is a simple app that calculates your final score in a class. It is designed to be used in the Korean education system, but it can be used in other education systems as well",
  "version": "23.3",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hslee2008/finalscalcu.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run .",
    "test:watch": "vitest .",
    "update": "yarn upgrade && ncu -u && yarn",
    "build:android": "cd android && bubblewrap build",
    "lint": "eslint . --ext .ts,.js,.svelte",
    "firebase-hosting-build": "yarn && yarn test && yarn lint && yarn build"
  },
  "devDependencies": {
    "@beyonk/svelte-google-analytics": "^2.6.4",
    "@sveltejs/vite-plugin-svelte": "^2.4.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/svelte": "^4.0.3",
    "@typescript-eslint/parser": "^6.1.0",
    "carbon-components-svelte": "^0.77.0",
    "carbon-icons-svelte": "^12.0.0",
    "carbon-preprocess-svelte": "^0.9.1",
    "eslint": ">=8.45.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "jsdom": "^22.1.0",
    "svelte": "^4.0.5",
    "svelte-eslint-parser": "^0.32.1",
    "svelte-meta-tags": "^2.8.0",
    "svelte-tabs": "^1.1.0",
    "vite": "^4.4.4",
    "vitest": "^0.33.0",
    "word-wrap": "^1.2.4"
  },
  "dependencies": {
    "firebase": "^10.0.0"
  }
}
